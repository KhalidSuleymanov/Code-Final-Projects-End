@model SpaService
@{
    ViewData["Title"] = "Create";
}
<div class="container">
    <div class="row justify-content-center">
        <div class="col-md-12">
            <hd class="mt-4"> Spa Service Create </hd>
        </div>
        <div class="col-md-12">
            <form method="post" asp-action="create" enctype="multipart/form-data">
                <div class="form-group mt-2">
                    <label asp-for="Name">Name</label>
                    <input asp-for="Name" class="form-control mt-2" style="border:1px solid #fff;">
                    <span asp-validation-for="Name"></span>
                </div>
                <div class="form-group mt-2">
                    <label asp-for="IconImage">IconImage</label>
                    <input asp-for="IconImage" class="form-control mt-2" style="border:1px solid #fff;">
                    <span asp-validation-for="IconImage"></span>
                </div>
                <button type="submit" class="btn btn-primary mt-4">Submit</button>
            </form>
        </div>
    </div>
</div>


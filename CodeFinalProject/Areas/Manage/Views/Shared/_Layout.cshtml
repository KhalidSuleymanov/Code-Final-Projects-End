<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>DarkPan - Bootstrap 5 Admin Template</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="~/Manage/Assets/img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;600&family=Roboto:wght@500;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/Manage/Assets/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Manage/Assets/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet" />

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/Manage/Assets/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/Manage/Assets/css/style.css" rel="stylesheet">
</head>

<body>
    <div class="container-fluid position-relative d-flex p-0">
        <!-- Spinner Start -->
        <div id="spinner" class="show bg-dark position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center">
            <div class="spinner-border text-primary" style="width: 3rem; height: 3rem;" role="status">
                <span class="sr-only">Loading...</span>
            </div>
        </div>
        <!-- Spinner End -->
        <!-- Sidebar Start -->
        <div class="sidebar pe-4 pb-3">
            <nav class="navbar bg-secondary navbar-dark">
                <a href="index.html" class="navbar-brand mx-4 mb-3">
                    <h3 class="text-primary"><i class="fa fa-user-edit me-2"></i>Tables</h3>
                </a>
                <div class="navbar-nav w-100">
                    <a asp-action="index" asp-controller="admin" class="nav-item nav-link">Admins</a>
                    <a asp-action="index" asp-controller="room" class="nav-item nav-link">Rooms</a>
                    <a asp-action="index" asp-controller="reservation" class="nav-item nav-link">Reserved Rooms</a>
                    <a asp-action="index" asp-controller="feature" class="nav-item nav-link">Features</a>
                    <a asp-action="index" asp-controller="staff" class="nav-item nav-link">Staffs</a>
                    <a asp-action="index" asp-controller="meal" class="nav-item nav-link">Meals</a>
                    <a asp-action="index" asp-controller="place" class="nav-item nav-link">Places</a>
                    <a asp-action="index" asp-controller="aboutservice" class="nav-item nav-link">About Services</a>
                    <a asp-action="index" asp-controller="branch" class="nav-item nav-link">Branchs</a>
                    <a asp-action="index" asp-controller="chef" class="nav-item nav-link">Chefs</a>
                    <a asp-action="index" asp-controller="gallery" class="nav-item nav-link">Galleries</a>
                    <a asp-action="index" asp-controller="ingredient" class="nav-item nav-link">Ingredients</a>
                    <a asp-action="index" asp-controller="service" class="nav-item nav-link">Services</a>
                    <a asp-action="index" asp-controller="spaservice" class="nav-item nav-link">SpaServices</a>
                    <a asp-action="index" asp-controller="blog" class="nav-item nav-link">Blogs</a>
                    <a asp-action="index" asp-controller="recentpost" class="nav-item nav-link">Recent Posts</a>
                    <a asp-action="index" asp-controller="award" class="nav-item nav-link">Awards</a>
                    <a asp-action="index" asp-controller="restaurant" class="nav-item nav-link">Restaurants</a>
                    <a asp-action="index" asp-controller="restaurantmodal" class="nav-item nav-link">Restaurant Modals</a>
                    <a asp-action="index" asp-controller="slider" class="nav-item nav-link">Sliders</a>
                    <a asp-action="index" asp-controller="contactus" class="nav-item nav-link">Contact Us</a>
                    <a asp-action="index" asp-controller="review" class="nav-item nav-link">Reviews</a>
                </div>
            </nav>
        </div>
        <!-- Sidebar End -->
        <!-- Content Start -->
        <div class="content">
            <!-- Navbar Start -->
            <nav class="navbar navbar-expand bg-secondary navbar-dark sticky-top px-4 py-0">
                <a href="index.html" class="navbar-brand d-flex d-lg-none me-4">
                    <h2 class="text-primary mb-0"><i class="fa fa-user-edit"></i></h2>
                </a>
                <a href="#" class="sidebar-toggler flex-shrink-0">
                    <i class="fa fa-bars"></i>
                </a>
                <form class="d-none d-md-flex ms-4">
                    <input class="form-control bg-dark border-0" type="search" placeholder="Search">
                </form>
                <div class="navbar-nav align-items-center ms-auto">
                    <div class="nav-item dropdown">
                        <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">
                            <img class="rounded-circle me-lg-2" src="~/Manage/Assets/img/user.jpg" alt="" style="width: 40px; height: 40px;">
                            <span class="d-none d-lg-inline-flex">John Doe</span>
                        </a>
                        <div class="dropdown-menu dropdown-menu-end bg-secondary border-0 rounded-0 rounded-bottom m-0">
                            <a href="#" class="dropdown-item">My Profile</a>
                            <a href="#" class="dropdown-item">Settings</a>
                            <a href="#" class="dropdown-item">Log Out</a>
                        </div>
                    </div>
                </div>
            </nav>
            @RenderBody()
        </div>
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/Manage/Assets/lib/chart/chart.min.js"></script>
    <script src="~/Manage/Assets/lib/easing/easing.min.js"></script>
    <script src="~/Manage/Assets/lib/waypoints/waypoints.min.js"></script>
    <script src="~/Manage/Assets/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/Manage/Assets/lib/tempusdominus/js/moment.min.js"></script>
    <script src="~/Manage/Assets/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="~/Manage/Assets/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>
    <script src="~/Manage/Assets/js/main.js"></script>
</body>

</html>